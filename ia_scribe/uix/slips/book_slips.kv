                bold: False
                font_size: root.default_font_size
            BookSlipLabel:
                id: boxid
                markup: True
                bold: False
                font_size: root.default_font_size
            BookSlipLabel:
                id: selector
                markup: True
                bold: False
                font_size: root.default_font_size

        BoxLayout:
            size_hint_y: None
            height: '150dp'
            spacing: '50dp'
            Image:
                size_hint_x: 0.5
